{
	"manifest_version": 2,

	"name": "Tab Manager",
	"description": "A simple tab manager",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": "false"
	},
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/",
		"tabs",
		"bookmarks"
	]
}
