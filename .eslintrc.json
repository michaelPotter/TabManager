{
    "env": {
        "browser": true,
        "webextensions": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
		"eslint-plugin-only-warn"
    ],
    "rules": {
		"no-unused-vars": ["off"],
		"no-unsafe-finally": ["off"]
    }
}
